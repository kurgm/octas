{
  "name": "octas",
  "version": "1.0.0",
  "description": "一筆書きゲーム",
  "main": "index.js",
  "scripts": {
    "browserify": "browserify lib/index.js --debug --transform [ babelify --presets [ es2015 ] ] | exorcist index.js.map > index.js",
    "pug": "pug index.pug",
    "less": "lessc index.less index.css",
    "build": "concurrently \"npm run browserify\" \"npm run pug\" \"npm run less\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kurgm/octas.git"
  },
  "author": "kurgm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kurgm/octas/issues"
  },
  "homepage": "https://github.com/kurgm/octas#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "concurrently": "^2.2.0",
    "exorcist": "^0.4.0",
    "gulp": "^3.9.1",
    "jquery": "^3.1.0",
    "less": "^2.7.1",
    "pug-cli": "^1.0.0-alpha6",
    "snapsvg": "^0.4.0"
  }
}
